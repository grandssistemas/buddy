{
	"extends": [
		"eslint:recommended",
		"airbnb-base"
	],
	"plugins": [
		"angular"
	],
	"globals": {
		"angular": true
	},
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module"
	},
	"env": {
		"browser": true,
		"es6": true,
		"jquery": false,
		"node": true
	},
	"rules": {
		"arrow-body-style": [
			1,
			"as-needed"
		],
		"arrow-parens": [
			1,
			"always"
		],
		"class-methods-use-this": 1,
		"comma-dangle": [
			1,
			"never"
		],
		"consistent-return": 1,
		"eol-last": 1,
		"func-names": 0,
		"import/extensions": 1,
		"import/imports-first": 1,
		"import/no-extraneous-dependencies": 1,
		"import/no-unresolved": 1,
		"indent": [
			1,
			"tab",
			{
				"SwitchCase": 1,
				"VariableDeclarator": 1
			}
		],
		"linebreak-style": "off",
		"max-len": "off",
		"new-cap": 1,
		"no-alert": 1,
		"no-console": [
			"warn",
			{
				"allow": [
					"error"
				]
			}
		],
		"object-shorthand": "warn",
		"no-debugger": 1,
		"no-global-assign": 1,
		"no-lone-blocks": 1,
		"no-lonely-if": 1,
		"no-multi-spaces": 1,
		"no-shadow": 1,
		"no-tabs": 0,
		"no-trailing-spaces": 1,
		"no-underscore-dangle": 1,
		"no-unsafe-negation": 1,
		"no-unused-vars": 1,
		"no-use-before-define": 1,
		"no-useless-constructor": 1,
		"no-var": 1,
		"object-curly-spacing": 1,
		"one-var": 1,
		"quotes": 1,
		"semi": [
			1,
			"always"
		],
		"prefer-destructuring": ["off"]
	}
}