String"},"19":{"Name":"total","Index":-1,"NameInSource":"total","Alias":"total","Type":"System.Double"}},"NameInSource":"Conexão","SqlCommand":"SELECT\r\n  distinct obj.id,\r\n  obj.sequencialcode,\r\n  CASE WHEN obj.status = 'FINISH' THEN 'FINALIZADO'\r\n       WHEN obj.status = 'REVERSED' THEN 'ESTORNADO'\r\n       WHEN obj.status = 'INTRANSFERENCE' THEN 'EM TRANFERENCIA'\r\n       WHEN obj.status = 'MISCARRIAGE' THEN 'EXTRAVIADO'\r\n       WHEN obj.status = 'CANCELLED' THEN 'CANCELADO'\r\n       WHEN obj.status = 'PENDING' THEN 'PENDENTE'\r\n       WHEN obj.status = 'BILLED_AGAIN' THEN 'REFATURADO'       \r\n       WHEN obj.status = 'INADJUSTMENT' THEN 'EM ACERTO'              \r\n       ELSE 'ABERTO'\r\n  END as status,\r\n  obj.movementdate,\r\n  e.serie,\r\n  e.nnf,\r\n  f.name,\r\n  f.nickname,\r\n  CASE WHEN g.cnpj <> '' THEN replace(to_char(g.cnpj::bigint, '00:000:000/0000-00'), ':', '.')\r\n       ELSE replace(to_char(cpf::bigint, '000:000:000-00'), ':', '.')\r\n  END as docFederal,\r\n  CASE WHEN g.stateregistration <> '' THEN g.stateregistration\r\n       ELSE h.docrg\r\n  END as docEstadual,\r\n  j.address_premisse_type || ' ' || j.address_premisse || ', ' || j.address_number as address,\r\n  j.address_neighbourhood,\r\n  j.address_localization || ' - ' || j.address_state as city,\r\n  j.address_zip_code,\r\n  j.description,\r\n  l.phone,\r\n  CASE WHEN obj.valueaddition <> 0 THEN obj.valueaddition\r\n       ELSE obj.valuediscount * -1\r\n  END as descontoAcrescimo,\r\n  obj.percentageDiscount,\r\n  abs(obj.total) total,\r\n  m.message\r\nFROM\r\n  fashionmanager.movementgroup obj\r\nLEFT JOIN fashionmanager.movementgroup_nfe b\r\n  ON obj.id = b.movementgroup_id\r\nLEFT JOIN fashionmanager.nfe c\r\n  ON b.nfelist_id = c.id\r\nLEFT JOIN fashionmanager.infnfe d\r\n  ON c.infnfe_id = d.id\r\nLEFT JOIN fashionmanager.ide e\r\n  ON d.ide_id = e.id\r\nINNER JOIN fashionmanager.person f\r\n  ON obj.targetperson_id = f.id\r\nLEFT JOIN fashionmanager.juridica g